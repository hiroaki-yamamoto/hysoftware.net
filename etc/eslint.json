{
  "extends": "eslint:recommended",
  "globals": {
    "angular": true
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "jasmine": true,
    "protractor": true,
    "jquery": true
  },
  "rules": {
      "max-len": ["error", 80, 2, {"ignoreUrls": true}],
      "indent": ["error", 2],
      "eol-last": ["error"],
      "no-eq-null": ["error"],
      "complexity": ["error", 10],
      "no-eval": ["error"],
      "no-script-url": ["error"],
      "radix": ["error"],
      "strict": ["error", "safe"]
  }
}
